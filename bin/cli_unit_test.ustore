# Is Branch Head
is_head -k basic -b master -v J3VZ2VRZO4BIMFNY5FAOU3QZKUM6R34B
is_head -k no-basic -b master -v J3VZ2VRZO4BIMFNY5FAOU3QZKUM6R34B
is_head -k basic -b no-master -v J3VZ2VRZO4BIMFNY5FAOU3QZKUM6R34B
