# Is Latest
is_latest -k basic -v HZ5ZUKAD2OVCIP6QAGUYZRMPCIZFNIZW
is_latest -k no-basic -v HZ5ZUKAD2OVCIP6QAGUYZRMPCIZFNIZW
is_latest -k basic -v J3VZ2VRZO4BIMFNY5FAOU3QZKUM6R34B
